
ApplicationControl {
  CheckParameter      = "Off"
  Operator            = "Kenji_Ono"
}


BcTable {

  LocalBoundary {

    cyl_1 {
      class             = "Obstacle"
      Medium            = "fe1"
    }

    cyl_2 {
      class             = "Obstacle"
      Medium            = "fe2"
    }

  }

  OuterBoundary {
    
    inlet {
      class       = "SpecifiedVelocity"
      Profile     = "Constant"
      OrientationVector = (1.0, 0.0, 0.0)
      velocity    = 1.0
      Medium      = "air"
      Temperature = 20
    }

    outlet {
      class        = "Outflow"
      Medium       = "air"
      PressureType = "dirichlet"
      PrsValue     = 0.0
    }

    outerWall {
      class       = "Wall"
      Type        = "fixed"
      Medium      = "fe"
    }

    slideWall {
      class       = "wall"
      Type        = "slide"
      Profile     = "Constant"
      OrientationVector = (1.0, 0.0, 0.0)
      Velocity    = 1.0
      Medium      = "fe"
    }

    symmetric {
      class       = "Symmetric"
      Medium      = "air"
    }
    
    periodic {
      class  = "periodic"
      mode   = "simplecopy"
    }

    FaceBC {
      Xminus = "inlet"
      Xplus  = "outlet"
      Yminus = "symmetric"
      Yplus  = "symmetric"
      Zminus = "symmetric"
      Zplus  = "symmetric"
    }

  }
}


ConvectionTerm {
  Scheme  = "O3MUSCL"
  Limiter = "minmod"
}


DomainInfo {
  UnitOfLength   = "NonDimensional"
  GlobalOrigin   = (-10.0, -10.0, 0.0)
  GlobalRegion   = (25.0,  20.0,  2.0    )
  //GlobalVoxel    = (64   , 64   , 3   )
  
  GlobalPitch    = (5.0e-02, 5.0e-02, 5.0e-02)
  //GlobalDivision = (1    , 1    , 1    )

}


FillHint {
  outer {
    kind      = "outerface"
    direction = "Xminus"
    medium    = "air"
  }
in_cyl1 {
kind       = "point"
coordinate = (0.0, 0.0, 0.2)
medium     = "fe1"
}

in_cyl2 {
kind       = "point"
coordinate = (3.0, 0.0, 0.1)
medium     = "fe2"
}

}


GeometryModel {
  Source                  = "cylinder"
  VoxelOutput             = "svx"
  OutputGlyph             = "on"
}


GoverningEquation {
  FlowEquation       = "Incompressible"
  HeatEquation       = "FlowOnly"
  Buoyancy           = "NoBuoyancy"
  TimeVariation      = "Unsteady"
  PDEType            = "NavierStokes" 
}


IntrinsicExample {
  FluidMedium = "air"
  Dimension   = "3d"

  Cylinder1 {
    UseCylinder = "yes"
    Shape = "rectangular"
    LengthX = 1.0
    LengthY = 1.0
    LengthZ = 0.5
    //Radius = 0.5
    PositionX = 0.0
    PositionY = 0.0
    SolidMedium = "fe1"
  }
  
  Cylinder2 {
    UseCylinder = "yes"
    Shape = "rectangular"
    LengthX = 1.0
    LengthY = 0.5
    LengthZ = 0.2
    //Radius = 0.3
    PositionX = 3.0
    PositionY = 0.0
    SolidMedium = "fe2"
  }

  Driver = 0.0
}


Iteration {
  LinearSolver[@] {
    Alias                = "sor2"
    class                = "sor2sma"
    MaxIteration         = 20
    ResidualCriterion    = 1.0e-4
    ResidualNorm         = "RbyB"
    ErrorNorm            = "DeltaXbyX"
    Omega                = 1.1
    CommMode             = "async"
    NaiveImplementation  = "off"
  }

  LinearSolver[@] {
    Alias                = "sor"
    class                = "sor"
    MaxIteration         = 20
    ResidualCriterion    = 1.0e-3
    ResidualNorm         = "RbyX"
    ErrorNorm            = "DeltaXbyX"
    Omega                = 1.1
  }

  LinearSolver[@] {
    Alias                = "bicg"
    class                = "bicgstab"
    MaxIteration         = 100
    ResidualCriterion    = 1.0e-6
    ResidualNorm         = "RbyB"
    ErrorNorm            = "DeltaXbyX"
	Precondition         = "on"
    InnerIteration       = 5
    Omega                = 1.1
    CommMode             = "async"
  }

  DivMaxIteration        = 100
  DivCriterion           = 1.0e-4
  DivNorm                = "max" // "L2"

  Pressure    = "bicg"
  Velocity    = ""
  Temperature = "sor2"     
}


