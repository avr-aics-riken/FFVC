
ApplicationControl {
  CheckParameter      = "Off"
  Operator            = "Kenji_Ono"
}


BcTable {

  OuterBoundary {
    
    inlet {
      class       = "SpecifiedVelocity"
      Profile     = "Constant"
      OrientationVector = (1.0, 0.0, 0.0)
      velocity    = 3.0
      Medium      = "air"
      Temperature = 20
    }

    outlet {
      class        = "Outflow"
      Medium       = "air"
      PressureType = "neumann"
      PrsValue     = 0.0
    }

    outerWall {
      class       = "Wall"
      Type        = "fixed"
      Medium      = "fe"
    }

    slideWall {
      class       = "wall"
      Type        = "slide"
      Profile     = "Constant"
      OrientationVector = (1.0, 0.0, 0.0)
      Velocity    = 1.0
      Medium      = "fe"
    }

    symmetric {
      class       = "Symmetric"
      Medium      = "air"
    }
    
    periodic {
      class  = "periodic"
      mode   = "simplecopy"
    }

    FaceBC {
      Xminus = "inlet"
      Xplus  = "outlet"
      Yminus = "symmetric"
      Yplus  = "symmetric"
      Zminus = "symmetric"
      Zplus  = "symmetric"
    }

  }
}


ConvectionTerm {
  Scheme  = "O3MUSCL"
  Limiter = "minmod"
}


DomainInfo {
  UnitOfLength   = "NonDimensional"
  GlobalOrigin   = (-10.0, -10.0, 0.0)
  GlobalRegion   = (25.0,  20.0,  0.15    )
  //GlobalVoxel    = (64   , 64   , 3   )
  
  GlobalPitch    = (5.0e-02, 5.0e-02, 5.0e-02)
  //GlobalDivision = (1    , 1    , 1    )

  ActiveSubDomainFile = ""

}


GeometryModel {
  Source                  = "cylinder"
  FillMedium              = "air"
  HintOfFillSeedDirection = "Xminus"
  HintOfFillSeedMedium    = "air"
  VoxelOutput             = "svx"
  FillDirectionControl    = ("fill", "fill", "suppress")
  OutputGlyph             = "off"
}


GoverningEquation {
  FlowEquation       = "Incompressible"
  HeatEquation       = "FlowOnly"
  Buoyancy           = "NoBuoyancy"
  TimeVariation      = "Unsteady"
  PDEType            = "NavierStokes" 
}


IntrinsicExample {
  FluidMedium = "air"
  Dimension   = "2d"

  Cylinder1 {
    UseCylinder = "yes"
    Shape = "rectangular"
    LengthX = 1.0
    LengthY = 1.0
    //LengthZ = 0.0
    //Radius = 0.5
    PositionX = 0.0
    PositionY = 0.0
    SolidMedium = "fe1"
  }
  
  Cylinder2 {
    UseCylinder = "yes"
    Shape = "circular"
    //LengthX = 0.4
    //LengthY = 1.0
    //LengthZ = 0.0
    Radius = 0.5
    PositionX = 2.0
    PositionY = 1.0
    SolidMedium = "fe2"
  }

  Driver = 0.0
}


Iteration {
  LinearSolver[@] {
    Alias                = "sor2_strict"
    class                = "sor2sma"
    MaxIteration         = 50
    ConvergenceCriterion = 1.0e-4
    NormType             = "RbyR0"
    Omega                = 1.1
    CommMode             = "async"
    NaiveImplementation  = "off"
  }

  LinearSolver[@] {
    Alias                = "sor2_loose"
    class                = "sor"
    MaxIteration         = 20
    ConvergenceCriterion = 1.0e-2
    NormType             = "RbyB"
    Omega                = 1.1
  }

  Pressure    = "sor2_strict"
  Velocity    = ""
  Temperature = "sor2_loose"   
}


